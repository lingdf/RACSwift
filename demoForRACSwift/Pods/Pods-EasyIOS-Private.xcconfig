#include "Pods-EasyIOS.xcconfig"
CLANG_CXX_LANGUAGE_STANDARD = ${PODS_EASYIOS_CLANG_CXX_LANGUAGE_STANDARD}
CLANG_CXX_LIBRARY = ${PODS_EASYIOS_CLANG_CXX_LIBRARY}
CLANG_WARN_DIRECT_OBJC_ISA_USAGE = ${PODS_EASYIOS_CLANG_WARN_DIRECT_OBJC_ISA_USAGE}
GCC_PREPROCESSOR_DEFINITIONS = COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/BuildHeaders" "${PODS_ROOT}/BuildHeaders/EasyIOS" "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/EasyIOS" "${PODS_ROOT}/Headers/ReactiveCocoa" "${PODS_ROOT}/Headers/ReactiveCocoa/ReactiveCocoa" ${PODS_EASYIOS_HEADER_SEARCH_PATHS}
OTHER_LDFLAGS = -ObjC ${PODS_EASYIOS_OTHER_LDFLAGS}
PODS_ROOT = ${SRCROOT}